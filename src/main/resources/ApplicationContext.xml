<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.example.ioc.Student" id="StdId1">
        <property name="name" value="Prasad"/>
        <property name="rollno" value="101"/>
        <property name="email" value="prasad@outlook.com"/>
    </bean>

    <bean class="com.example.ioc.Student" id="StdId2">
        <property name="name" value="Amit"/>
        <property name="rollno" value="102"/>
        <property name="email" value="amit@outlook.com"/>
    </bean>

</beans>
